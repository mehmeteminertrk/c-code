#include <stdio.h>

int main()
{
   int x,y,z,sayi_total;
   printf("Yuzler basamagini giriniz:");
   scanf("%d",&x);
   printf("Onlar basamagini giriniz:");
   scanf("%d",&y);
   printf("Birler basamagini giriniz:");
   scanf("%d",&z);
   sayi_total=100*x+10*y+z;
  printf("%d",sayi_total);
    return 0;
}
