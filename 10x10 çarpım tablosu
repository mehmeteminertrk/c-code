#include <stdio.h>
#define anonim 10

int main()
{
  int i,j;
  for(i=1;i<=anonim;i++){
      
      printf("%4d",i);
  }
  printf("\n");
  for(i=1;i<=anonim;i++){
      
      printf("-----");
  }
  
  
  printf("\n");
  for(i=1;i<anonim;i++){
      printf("%4d",i);
      for(j=1;j<=anonim;j++){
          printf("%4d",i*j);
      }
      printf("\n");
  }

    return 0;
}
