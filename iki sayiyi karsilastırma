#include <stdio.h>

int main()
{
   int a,b;
   printf("Lutfen ilk sayiyi giriniz:\n");
   scanf("%d",&a);
   printf("Lutfen ikinci sayiyi giriniz:\n");
   scanf("%d",&b);
   if(a==b){
       printf("İki sayi birbirine esittir\n");
   }
   if(a!=b){
       printf("İki sayi birbirine esit degildir\n");
   }
   if(a<b){
       printf("İlk sayi ikinci sayidan kucuktur\n");
   }
   
   if(a>b){
       printf("İlk sayi ikinci sayidan buyuktur\n");
   }
   if(a%b==0){
       printf("İlk sayi ikinci sayinin katıdır\n");
   }
   if(a%b!=0){
       printf("İlk sayi ikinci sayinin katıdı degildir\n");
   }
   if(b%a==0){
       printf("ikinci sayi İlk sayinin katıdır");
   }
   
   
    return 0;
}
