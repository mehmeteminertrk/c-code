#include <stdio.h>

int main()
{
   int a,x,b,c;
   int sonuc;
   printf("Lutfen a,b,c ve x degerlerini giriniz:\n");
   scanf("%d%d%d%d",&a,&b,&c,&x);
   sonuc=a*x*x+b*x+c;
   printf("%d",sonuc);

    return 0;
}
