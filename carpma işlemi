#include <stdio.h>

int main()
{
  int x,y;
  int carpim;
  printf("Lutfen iki sayi giriniz:\n");
  scanf("%d%d",&x,&y);
  carpim+=x*y;
  if((x==0) && (y==0)){
      printf("Carpim sifirdir",carpim);
  }
  else if((x<0) && (y<0)){
      printf("Carpim pozitiftir:%d dir",carpim);
  }
  else if((x<0) && (y>0)){
      printf("Carpim negatiftir:%d dir",carpim);
  }
  else if((x>0) && (y<0)){
      printf("Carpim negatiftir:%d dir",carpim);
  }
    
   else if((x==0) && (y!=0)){
      printf("Carpim sifirdir",carpim);
  }
  else{
      printf("Carpim sifirdir");
  }
    return 0;
    
}
