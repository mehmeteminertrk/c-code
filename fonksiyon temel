#include <string.h>
#include <stdio.h>
int tekmiciftmi(int emin)
{
    if(emin%2==1){
      return 1;
  }
  else{
      return 0;
  }
}
int main()
{
  int sayi,sonuc;
  printf("Lutfen Sayi giriniz:\n");
  scanf("%d",&sayi);
  
   sonuc=tekmiciftmi(sayi);
    if(sonuc==1){
        printf("Sayimiz tektir");
    }
    if(sonuc==0){
        printf("Sayimiz cifttir");
    }
    
    return 0;
}
