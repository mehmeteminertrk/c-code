#include <stdio.h>

int main()
{
   char massaka;
   printf("Lutfen bir karekter giriniz:\n");
   scanf("%c",&massaka);
   
   if((massaka>='A')&&(massaka<='Z')){
       printf("Girilen karekterin kucuk hali:%c\n",tolower(massaka));
   }
   
   else{
       printf("Lutfen Buyuk karekter giriniz");
   }
   
   
   
   
    return 0;
}
