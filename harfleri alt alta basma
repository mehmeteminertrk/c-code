#include <stdio.h>

int main()
{
    char dizi[100];
    int i;
    printf("Lutfen bir cumle giriniz:\n");
    gets(dizi);
    for(i=0;dizi[i];i++){
        printf("%c\n",dizi[i]);
        
    }
    return 0;
}
