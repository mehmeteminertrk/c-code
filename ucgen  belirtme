#include <stdio.h>

int main()
{
    int aci1,aci2,aci3;
    printf("Lutfen acilarini giriniz:\n");
    scanf("%d%d%d",&aci1,&aci2,&aci3);
    if(aci1+aci2+aci3==180){
        if(aci1==60 && aci2==60 && aci3==60){
            printf("Bu bir eskenar ucgendir\n");
        }
        else if(aci1==aci2 && aci2!=aci3){
            printf("Bu bir ikizkenar ucgendir");
        }
        else if(aci2==aci3 && aci1!=aci2){
            printf("Bu bir ikizkenar ucgendir");
        }
        else if(aci1==aci3 && aci1!=aci2){
            printf("Bu bir ikizkenar ucgendir");
        }
        else{
            printf("Bu bir cesitkenar ucgendir");
        }
    }
else {
    printf("AcÄ±lar ucgen belirtmez");
}
    return 0;
}
