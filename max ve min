#include <stdio.h>
#define anonim 3
int main()
{
    int max,min;
    int i;
    int dizi[anonim];
    printf("Lutfen %d adet sayi giriniz:\n",anonim);
    
    for(i=0;i<anonim;i++){
        scanf("%d",&dizi[i]);
    }
    max=min=dizi[0];
    for(i=1;i<anonim;i++){
        if(dizi[i]>max){
            max=dizi[i];
        }
        if(dizi[i]<min){
            min=dizi[i];
        }
    }
   
    
printf("Maksimum deger:%d\n",max);
printf("Minimum deger:%d",min);
  
    return 0;
}
