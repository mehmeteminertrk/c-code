#include <stdio.h>

int main()
{
    int i,limit,satir;
    printf("Lutfen bir limit degeri  giriniz:");
    scanf("%d",&limit);
    i=0;
    satir=0;
    while(i<=limit){
        if(i%17==0){
            
            printf("%5d",i);
             if(satir%10==0){
            printf("\n");
        }
        satir++;
        }
       
        
        i++;
       
    }

    return 0;
}
